# Updates Fresh
fresh freshshell/fresh bin/fresh --bin=~/.local/bin/fresh

# Git
fresh config/gitconfig --file=~/.gitconfig
fresh ../.gitconfig.local --file=~/.gitconfig
fresh config/gitcommit --file
fresh config/gitignore --file

# ZSH
fresh config/zshrc.min --file
fresh config/zshrc --file
fresh config/zshenv.min --file
fresh config/zshenv --file

# Tmux
fresh config/tmux.conf --file

# Shell
fresh shell/aliases/\*
fresh shell/functions/\*

# vim
fresh config/vimrc --file=~/.vimrc
fresh config/vimrc.min --file=~/.vimrc.min

# Alacritty
fresh config/alacritty.yml --file

# Idea Vim
fresh config/ideavimrc --file

# GPG
fresh config/gpg.conf --file ~/.gnupg/gpg.conf
fresh config/gpg-agent.conf --file ~/.gnupg/gpg-agent.conf

# Bin
fresh bin/toggle-dnd --bin=~/.local/bin/toggle-dnd

# SSH
fresh config/ssh --file ~/.ssh/config
